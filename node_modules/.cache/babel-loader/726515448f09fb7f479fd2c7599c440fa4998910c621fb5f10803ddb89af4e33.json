{"ast":null,"code":"export default {\n  props: [\"selectedContactInfo\", \"contacts\", \"formType\"],\n  data() {\n    return {\n      searchQuery: '',\n      searchResults: [],\n      selectedContact: null\n    };\n  },\n  computed: {\n    computedSelectedContact() {\n      return this.formType ? this.selectedContactInfo : this.selectedContact;\n    }\n  },\n  methods: {\n    setSelectedContact(result) {\n      this.selectedContact = result;\n      this.$emit('setContact', result);\n      this.searchResults = [];\n    },\n    async search() {\n      try {\n        this.searchResults = this.contacts.filter(item => item.fields.contact_name.toLowerCase().includes(this.searchQuery.toLowerCase()) || item.fields.contact_email.toLowerCase().includes(this.searchQuery.toLowerCase()));\n      } catch (error) {\n        console.error('Error fetching data:', error);\n        // Hata durumunu ele almak için uygun işlemler buraya eklenebilir\n      }\n    }\n  },\n  watch: {\n    searchQuery: {\n      handler() {\n        this.search();\n        if (this.searchQuery == '') {\n          this.searchResults = [];\n        }\n      },\n      immediate: true\n    }\n  }\n};","map":{"version":3,"names":["props","data","searchQuery","searchResults","selectedContact","computed","computedSelectedContact","formType","selectedContactInfo","methods","setSelectedContact","result","$emit","search","contacts","filter","item","fields","contact_name","toLowerCase","includes","contact_email","error","console","watch","handler","immediate"],"sources":["src/components/ContactSearch.vue"],"sourcesContent":["<template>\n    <div class=\"contact-search\">\n        <b-input-group v-if=\"formType != 'edit' && \">\n            <b-form-input type=\"text\" v-model=\"searchQuery\" class=\"text-center\" placeholder=\"Search\"></b-form-input>\n            <b-input-group-append>\n                <b-button variant=\"dark\">\n                    <b-icon icon=\"search\" font-scale=\"0.9\"></b-icon>\n                </b-button>\n            </b-input-group-append>\n        </b-input-group>\n        <ul class=\"search-results\" v-if=\"searchResults && searchResults.length > 0\">\n            <li v-for=\"result in searchResults\" :key=\"result.id\">\n                <div @click=\"setSelectedContact(result)\" class=\"search-list-item\">\n                    <span class=\"d-block\">\n                        {{ result.fields.contact_name }} {{ result.fields.contact_surname }}\n                    </span>\n                    <span class=\"d-block\">\n                        {{ result.fields.contact_email }}\n                    </span>\n                </div>\n            </li>\n        </ul>\n        <div class=\"contact-info-area position-relative\" v-if=\"computedSelectedContact\">\n            <div class=\"remove-contact-btn position-absolute\"\n             style=\"top: 0px; right: 0px;\">\n                <b-button variant=\"transparent\">\n                    <b-icon icon=\"x\"></b-icon>\n                </b-button>\n            </div>\n            <div class=\"contact-name mb-2\">\n                <b-icon icon=\"person\" font-scale=\"1.1\" class=\"me-2\"></b-icon>\n                <b>{{ computedSelectedContact.fields.contact_name + ' ' + computedSelectedContact.fields.contact_surname\n                    }}</b>\n            </div>\n\n            <div class=\"contact-mail mb-2\">\n                <b-icon icon=\"mailbox\" font-scale=\"1.1\" class=\"me-2\"></b-icon>\n                {{ computedSelectedContact.fields.contact_email }}\n            </div>\n\n            <div class=\"contact-phone mb-2\">\n                <b-icon icon=\"phone\" font-scale=\"1.1\" class=\"me-2\"></b-icon>\n                {{ computedSelectedContact.fields.contact_phone }}\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nexport default {\n    props: [\"selectedContactInfo\", \"contacts\", \"formType\"],\n    data() {\n        return {\n            searchQuery: '',\n            searchResults: [],\n            selectedContact: null\n        }\n    },\n    computed: {\n        computedSelectedContact() {\n            return this.formType ? this.selectedContactInfo : this.selectedContact;\n        }\n    },\n    methods: {\n        setSelectedContact(result) {\n            this.selectedContact = result;\n            this.$emit('setContact', result)\n            this.searchResults = []\n        },\n        async search() {\n            try {\n                this.searchResults = this.contacts.filter(item =>\n                    item.fields.contact_name.toLowerCase().includes(this.searchQuery.toLowerCase()) ||\n                    item.fields.contact_email.toLowerCase().includes(this.searchQuery.toLowerCase())\n                );\n            } catch (error) {\n                console.error('Error fetching data:', error);\n                // Hata durumunu ele almak için uygun işlemler buraya eklenebilir\n            }\n        }\n    },\n    watch: {\n        searchQuery: {\n            handler() {\n                this.search();\n                if (this.searchQuery == '') {\n                    this.searchResults = []\n                }\n            },\n            immediate: true\n        }\n    }\n};\n</script>\n"],"mappings":"AAiDA;EACAA,KAAA;EACAC,KAAA;IACA;MACAC,WAAA;MACAC,aAAA;MACAC,eAAA;IACA;EACA;EACAC,QAAA;IACAC,wBAAA;MACA,YAAAC,QAAA,QAAAC,mBAAA,QAAAJ,eAAA;IACA;EACA;EACAK,OAAA;IACAC,mBAAAC,MAAA;MACA,KAAAP,eAAA,GAAAO,MAAA;MACA,KAAAC,KAAA,eAAAD,MAAA;MACA,KAAAR,aAAA;IACA;IACA,MAAAU,OAAA;MACA;QACA,KAAAV,aAAA,QAAAW,QAAA,CAAAC,MAAA,CAAAC,IAAA,IACAA,IAAA,CAAAC,MAAA,CAAAC,YAAA,CAAAC,WAAA,GAAAC,QAAA,MAAAlB,WAAA,CAAAiB,WAAA,OACAH,IAAA,CAAAC,MAAA,CAAAI,aAAA,CAAAF,WAAA,GAAAC,QAAA,MAAAlB,WAAA,CAAAiB,WAAA,GACA;MACA,SAAAG,KAAA;QACAC,OAAA,CAAAD,KAAA,yBAAAA,KAAA;QACA;MACA;IACA;EACA;EACAE,KAAA;IACAtB,WAAA;MACAuB,QAAA;QACA,KAAAZ,MAAA;QACA,SAAAX,WAAA;UACA,KAAAC,aAAA;QACA;MACA;MACAuB,SAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"m