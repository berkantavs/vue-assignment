nc created() {\n        if (this.contacts) {\n            this.selectedContact = this.contacts.filter(contact => contact.id == this.selectedContactId)\n        }\n    },\n    computed: {\n        agentList() {\n            return this.agents.map(agent => ({\n                id: agent.id,\n                label: `${agent.fields.agent_name} ${agent.fields.agent_surname}`,\n            }));\n        },\n    },\n    methods: {\n        setContact(contact) {\n            this.selectedContact = contact;\n        },\n        async submitAppointment() {\n            console.log('tst');\n        },\n    }\n};\n</script>"],"mappings":"AA+BA,OAAAA,aAAA;AACA,OAAAC,SAAA;AACA;AACA,OAAAC,SAAA;AACA;AACA;EACAC,KAAA;EACAC,UAAA;IACAJ,aAAA;IACAC,SAAA;IACAI,SAAA,EAAAH;EACA;EACAI,KAAA;IACA;MACAC,eAAA;MACAC,kBAAA;MACAC,aAAA;MACAC,eAAA;MACAC,iBAAA;MACAC,MAAA;QACAC,UAAA;QACAC,SAAA;MACA;IACA;EACA;EACA,MAAAC,QAAA;IACA,SAAAC,QAAA;MACA,KAAAT,eAAA,QAAAS,QAAA,CAAAC,MAAA,CAAAC,OAAA,IAAAA,OAAA,CAAAC,EAAA,SAAAR,iBAAA;IACA;EACA;EACAS,QAAA;IACAC,UAAA;MACA,YAAAC,MAAA,CAAAC,GAAA,CAAAC,KAAA;QACAL,EAAA,EAAAK,KAAA,CAAAL,EAAA;QACAM,KAAA,KAAAD,KAAA,CAAAE,MAAA,CAAAC,UAAA,IAAAH,KAAA,CAAAE,MAAA,CAAAE,aAAA;MACA;IACA;EACA;EACAC,OAAA;IACAC,WAAAZ,OAAA;MACA,KAAAX,eAAA,GAAAW,OAAA;IACA;IACA,MAAAa,kBAAA;MACAC,OAAA,CAAAC,GAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}