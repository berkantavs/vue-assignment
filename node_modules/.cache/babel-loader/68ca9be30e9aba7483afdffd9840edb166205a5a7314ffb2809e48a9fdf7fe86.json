onst now = new Date();\n            if (this.record.fields.is_cancelled) {\n                this.recordStatus = 'Cancelled';\n                this.recordStatusColor = '#ff0000';\n                this.recordRemainingText = ''\n                return;\n            } else if (end > now) {\n                this.recordStatus = 'Upcoming';\n                this.recordStatusColor = '#ffa500';\n                return;\n            } else {\n                this.recordStatus = 'Completed';\n                this.recordStatusColor = '#008000';\n                this.recordRemainingText = ''\n            }\n        },\n        formatAppointmentDate(dateString) {\n            const date = new Date(dateString);\n            const day = String(date.getUTCDate()).padStart(2, '0');\n            const month = String(date.getUTCMonth() + 1).padStart(2, '0');\n            const year = date.getUTCFullYear();\n            const hours = String(date.getUTCHours()).padStart(2, '0');\n            const minutes = String(date.getUTCMinutes()).padStart(2, '0');\n\n            return `${day}/${month}/${year} ${hours}:${minutes}`;\n        }\n    }\n};\n</script>\n"],"mappings":"AA0DA,OAAAA,WAAA;AACA,OAAAC,gBAAA;AACA;EACAC,KAAA;EACAC,UAAA;IAAAH,WAAA;IAAAC;EAAA;EACAG,KAAA;IACA;MACAC,YAAA;MACAC,iBAAA;MACAC,mBAAA;IACA;EACA;EACAC,QAAA;IACAC,qBAAA;MACA,UAAAC,MAAA,UAAAA,MAAA,CAAAC,MAAA,UAAAD,MAAA,CAAAC,MAAA,CAAAC,QAAA,UAAAC,MAAA;QACA;MACA;MACA,YAAAH,MAAA,CAAAC,MAAA,CAAAC,QAAA,CAAAE,GAAA,CAAAC,OAAA;QACA,MAAAC,KAAA,QAAAH,MAAA,CAAAI,IAAA,CAAAD,KAAA,IAAAA,KAAA,CAAAE,EAAA,KAAAH,OAAA;QACA;UACAJ,MAAA;YACAQ,UAAA,EAAAH,KAAA,EAAAL,MAAA,EAAAQ,UAAA;YACAC,aAAA,EAAAJ,KAAA,EAAAL,MAAA,EAAAS,aAAA;YACAR,QAAA,EAAAI,KAAA,CAAAE,EAAA;YACAG,KAAA,EAAAL,KAAA,EAAAL,MAAA,EAAAU,KAAA;UACA;QACA;MACA;IACA;EACA;EACAC,KAAA;IACAZ,MAAA;MACAa,SAAA;MACAC,QAAAC,QAAA,EAAAC,QAAA;QACA,IAAAD,QAAA,KAAAC,QAAA;UACA,KAAAC,6BAAA;QACA;MACA;IACA;EACA;EACAC,OAAA;IACAC,UAAA;MACA,KAAAC,QAAA,CAAAC,IAAA,6BAAArB,MAAA,CAAAQ,EAAA;IACA;IACAS,8BAAA;MACA,MAAAK,K