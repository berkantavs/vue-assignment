 (index !== -1) {\n        this.selectedAgents.splice(index, 1);\n      }\n    },\n    async fetchAppointments() {\n      const tableName = 'Appointments';\n      try {\n        this.records = await fetchRecords(tableName);\n        this.records.sort((a, b) => new Date(b.fields.appointment_date) - new Date(a.fields.appointment_date));\n        console.log(this.records);\n\n      } catch (error) {\n        console.error('Error fetching data:', error);\n      }\n    },\n    async fetchAgents() {\n      const tableName = 'Agents';\n      try {\n        this.agents = await fetchAgents(tableName);\n        console.log(this.agents);\n\n      } catch (error) {\n        console.error('Error fetching data:', error);\n      }\n    },\n  }\n};\n</script>\n"],"mappings":";AAiFA,SAAAA,YAAA;AACA,SAAAC,WAAA;AACA,OAAAC,WAAA;AACA,OAAAC,oBAAA;AACA,OAAAC,QAAA;AAEA;EACAC,UAAA;IACAH,WAAA;IACAE,QAAA;IACAD;EACA;EACAG,KAAA;IACA;MACAC,OAAA;MACAC,MAAA;MACAC,cAAA;MACAC,cAAA;MACAC,QAAA,GACA;QAAAC,KAAA;QAAAC,IAAA;MAAA,GACA;QAAAD,KAAA;QAAAC,IAAA;MAAA,GACA;QAAAD,KAAA;QAAAC,IAAA;MAAA,GACA;QAAAD,KAAA;QAAAC,IAAA;MAAA,EACA;MACAC,eAAA;MACAC,aAAA;MACAC,WAAA;MACAC,YAAA;IACA;EACA;EAEAC,QAAA;IACAC,WAAA;MACA,OAAAC,IAAA,CAAAC,IAAA,MAAAd,OAAA,CAAAe,MAAA,QAAAL,YAAA;IACA;IACA;IACA;IACA;IACA;IACA;EACA;EAEA,MAAAM,QAAA;IACA,WAAAC,iBAAA;IACA,WAAAvB,WAAA;EACA;EAEAwB,OAAA;IACAC,iBAAAC,KAAA;MACA,UAAAlB,cAAA,CAAAmB,QAAA,CAAAD,KAAA;QACA,KAAAlB,cAAA,CAAAoB,IAAA,CAAAF,KAAA;MACA;IACA;IACAG,mBAAAH,KAAA;MACA,MAAAI,KAAA,QAAAtB,cAAA,CAAAuB,OAAA,CAAAL,KAAA;MACA,IAAAI,KAAA;QACA,KAAAtB,cAAA,CAAAwB,MAAA,CAAAF,KAAA;MACA;IACA;IACA,MAAAP,kBAAA;MACA,MAAAU,SAAA;MACA;QACA,KAAA3B,OAAA,SAAAP,YAAA,CAAAkC,SAAA;QACA,KAAA3B,OAAA,CAAA4B,IAAA,EAAAC,CAAA,EAAAC,CAAA,SAAAC,IAAA,CAAAD,CAAA,CAAAE,MAAA,CAAAC,gBAAA,QAAAF,IAAA,CAAAF,CAAA,CAAAG,MAAA,CAAAC,gBAAA;QACAC,OAAA,CAAAC,GAAA,MAAAnC,OAAA;MAEA,SAAAoC,KAAA;QACAF,OAAA,CAAAE,KAAA,yBAAAA,KAAA;MACA;IACA;IACA,MAAA1C,YAAA;MACA,MAAAiC,SAAA;MACA;QACA,KAAA1B,MAAA,SAAAP,WAAA,CAAAiC,SAAA;QACAO,OAAA,CAAAC,GAA